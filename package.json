{
  "name": "cosmjs-stargate-playground",
  "version": "1.0.0",
  "description": "Evelyn's playground for Tendermint CosmosSDK Chain (CosmosJS | Stargate)",
  "main": "index.js",
  "scripts": {
    "prettier-config": "prettier '*/**/*.{sol,json}' '*.{sol,json}'",
    "prettier:check": "yarn prettier-config --check",
    "prettier:fix": "yarn prettier-config --write",
    "eslint:check": "eslint '*/**/*.{js,ts}' '*.{js,ts}'",
    "eslint:fix": "yarn eslint:check --fix",
    "format:check": "yarn prettier:check && yarn eslint:check",
    "format": "yarn eslint:fix",
    "script": "node -r ts-node/register --require ./configs.ts scripts/main.ts"
  },
  "author": "Eve",
  "license": "ISC",
  "devDependencies": {
    "chain-registry": "git+https://github.com/cosmos/chain-registry.git",
    "@types/chai": "4.2.22",
    "@types/chai-as-promised": "7.1.4",
    "@types/mocha": "9.0.0",
    "@types/sinon": "10.0.5",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "prettier": "2.4.1",
    "sinon": "11.1.2",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@cosmjs/stargate": "^0.28.7",
    "@types/node": "16.11.4",
    "bignumber.js": "9.0.1"
  }
}
